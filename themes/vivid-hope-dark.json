      "settings": {
        "foreground": "#c8c8c8"
      }
    },
    {
      "name": "Parameters",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#aa78ff"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#1e78ff"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "variable.other.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#c8c8c8"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": "meta.object-literal.key",
      "settings": {
