{
  "editor.formatOnSave": false,
  "editor.rulers": [145],
  "prettier.printWidth": 145,
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/**/node_modules": true
  },
  "search.exclude": {
    ".git/": true,
    "_ignore/": true,
    "node_modules/": true,
    "package-lock.json": true
  }
}
